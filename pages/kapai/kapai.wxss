.header{
    text-align: center;
    font-size: 140%;
    font-weight: bold;
    color: #1F4499;
    padding: 10rpx;
}
.author{
    text-align: center;
    color: #3273dc;
    font-size: 80%;
}
button{
    background-color: #3298dc !important;
}
picker, input{
    display: inline-block;
    padding: 5rpx 10rpx;
    border-radius: 4rpx;
    border: 1px solid lightgray;
}
radio-group{
    text-align: center;
    margin: 10rpx;
}
.radio{
    margin: 0 5rpx;
}
.m10{
    margin: 0 10rpx;
}
