    margin: 10rpx 0 !important;
    background-color: cornflowerblue;
}
.guancangbtns{
    display: flex;
    justify-content: space-evenly;
}
.collection .coll-item{
    display: flex;
    justify-content: space-between;
    margin: 10rpx 150rpx;
}
.collection .coll-item .gcName{
